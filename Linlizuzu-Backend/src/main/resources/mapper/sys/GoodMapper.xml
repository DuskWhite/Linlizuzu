<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE mapper PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN" "http://mybatis.org/dtd/mybatis-3-mapper.dtd">
<mapper namespace="com.xyxy.sys.mapper.GoodMapper">
    <update id="updateGoodStatus" parameterType="Integer" >
        UPDATE zu_good
        SET status = 1
        WHERE
            id = #{id}
    </update>
</mapper>
